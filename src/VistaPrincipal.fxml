<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<SplitPane dividerPositions="0.5" minHeight="360.0" minWidth="440.0" prefHeight="360.0" prefWidth="440.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="VistaPrincipalController">
   <items>
      <VBox prefHeight="398.0" prefWidth="326.0">
         <children>
            <Pane prefHeight="200.0" prefWidth="200.0" translateX="10.0">
               <children> 
                   <fx:include fx:id="componenteDatos" source="componenteDatos/ComponenteDatos.fxml" />
                  
               </children>
            </Pane>
            <Button fx:id="btGenerarContrasena" alignment="CENTER" mnemonicParsing="false" onAction="#generarContrasena" prefHeight="25.0" prefWidth="140.0" text="Generar contraseña" translateX="35.0" />
            <Button fx:id="btNuevosDatos" minHeight="-Infinity" mnemonicParsing="false" onAction="#nuevosDatos" prefHeight="25.0" prefWidth="140.0" text="Nuevos datos" translateX="35.0" translateY="15.0" />
         </children>
      </VBox>
      <VBox prefHeight="200.0" prefWidth="100.0">
         <children>
            <Label text="Contraseña generada" translateX="20.0" translateY="5.0" />
            <TextField fx:id="contrasena" maxWidth="-Infinity" minWidth="-Infinity" prefHeight="25.0" prefWidth="180.0" translateX="20.0" translateY="10.0" />
            <Button fx:id="btSalir" mnemonicParsing="false" onAction="#salir" prefHeight="25.0" prefWidth="140.0" text="Salir" translateX="40.0" translateY="30.0" />
         </children>
      </VBox>
   </items>
</SplitPane>
